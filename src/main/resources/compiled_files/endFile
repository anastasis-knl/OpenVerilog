    .scope S_0x57c98ff39560;
T_5 ;
    %vpi_call 2 33 "$monitor", "At time %t, value = %h (%0d)", $time, v0x57c98ff4d880_0, v0x57c98ff4d880_0 {0 0 0};
    %end;
    .thread T_5;

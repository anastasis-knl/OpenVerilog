 .timescale 0 0;
    .port_info 0 /OUTPUT 8 "out";
    .port_info 1 /INPUT 1 "clk";
    .port_info 2 /INPUT 1 "reset";
P_0x57c98ff38e50 .param/l "WIDTH" 0 3 3, +C4<00000000000000000000000000001000>;
v0x57c98ff38f60_0 .net "clk", 0 0, v0x57c98ff4d710_0;  1 drivers
v0x57c98ff4d510_0 .var "out", 7 0;
v0x57c98ff4d5f0_0 .net "reset", 0 0, v0x57c98ff4d7b0_0;  1 drivers
E_0x57c98fefffa0 .event posedge, v0x57c98ff4d5f0_0, v0x57c98ff38f60_0;
S_0x57c98ff396f0 .scope module, "tst" "tst" 4 1;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "out";
o0x7de5fa524138 .functor BUFZ 1, C4<z>; HiZ drive
v0x57c98ff4d980_0 .net "out", 0 0, o0x7de5fa524138;  0 drivers
    .scope S_0x57c98ff38cc0;
T_0 ;
    %wait E_0x57c98fefffa0;
    %load/vec4 v0x57c98ff4d5f0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x57c98ff4d510_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x57c98ff4d510_0;
    %addi 1, 0, 8;
    %assign/vec4 v0x57c98ff4d510_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x57c98ff39560;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57c98ff4d7b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57c98ff4d710_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x57c98ff39560;
T_2 ;
    %delay 17, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57c98ff4d7b0_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57c98ff4d7b0_0, 0, 1;
    %delay 29, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x57c98ff4d7b0_0, 0, 1;
    %delay 11, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x57c98ff4d7b0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 13 "$stop" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x57c98ff39560;
T_3 ;
    %delay 5, 0;
    %load/vec4 v0x57c98ff4d710_0;
    %nor/r;
    %store/vec4 v0x57c98ff4d710_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x57c98ff39560;
T_4 ;
    %vpi_call 2 25 "$dumpfile", "dump.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars" {0 0 0};
    %delay 1000, 0;
    %vpi_call 2 28 "$display", "Caught by trap" {0 0 0};
